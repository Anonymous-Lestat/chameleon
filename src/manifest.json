{
	"author": "sereneblue",
	"applications": {
		"gecko": {
			"id": "{3579f63b-d8ee-424f-bbb6-6d0ce3285e6a}",
			"strict_min_version": "59.0a1"
		}
	},
	"name": "Chameleon",
	"version": "0.12.0",
	"manifest_version": 2,
	"description": "Spoof your browser profile. Includes a few privacy enhancing options.",
	"background": {
		"scripts": [
			"js/dep/jquery-3.3.1.slim.min.js",
			"js/dep/ip-cidr.min.js",
			"js/dep/moment.min.js",
			"js/dep/moment-timezone-with-data.min.js",
			"js/dep/psl.min.js", 
			"js/lib/spoof.js",
			"js/lib/spoofAudioContext.js",
			"js/lib/spoofrects.js",
			"js/lib/spooftime.js",
			"js/data.js", 
			"js/inject.js",
			"js/background.js"
		]
	},
	"browser_action": {
		"default_icon": "img/icon_48.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "img/icon_48.png",
		"128": "img/icon_128.png"
	},
	"permissions": [
		"<all_urls>",
		"alarms",
		"notifications",
		"privacy",
		"storage",
		"tabs",
		"webRequest",
		"webRequestBlocking"
	]
}